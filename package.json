{
  "name": "batchprot",
  "version": "0.0.1",
  "private": true,
  "engines": {
    "node": ">=22.14.0",
    "pnpm": ">=10.11.0"
  },
  "type": "module",
  "packageManager": "pnpm@10.13.1+sha512.37ebf1a5c7a30d5fabe0c5df44ee8da4c965ca0c5af3dbab28c3a1681b70a256218d05c81c9c0dcf767ef6b8551eb5b960042b9ed4300c59242336377e01cfad",
  "scripts": {
    "analyze": "turbo analyze",
    "boundaries": "turbo boundaries",
    "build": "turbo build",
    "bump-deps": "npx npm-check-updates --deep -u -x react-day-picker",
    "bump-ui": "npx shadcn@latest add --all --overwrite -c packages/design-system",
    "clean": "git clean -xdf node_modules",
    "clean:workspaces": "turbo run clean",
    "db:generate": "turbo -F @repo/database dk:generate",
    "db:migrate": "turbo -F @repo/database migrate",
    "db:pull": "turbo -F @repo/database pull",
    "db:push": "turbo -F @repo/database push",
    "db:studio": "turbo -F @repo/database studio",
    "dev": "sst dev",
    "db:migrate:prod": "turbo -F @repo/database migrate:prod",
    "db:push:prod": "turbo -F @repo/database push:prod",
    "db:studio:prod": "turbo -F @repo/database studio:prod",
    "sst:deploy": "sst deploy --stage production",
    "format": "ultracite format",
    "lint": "ultracite lint",
    "lint:ws": "pnpm dlx sherif@latest",
    "postinstall": "pnpm lint:ws",
    "start:db": "./start-database.sh",
    "test": "turbo test",
    "tunnel:prod": "sst tunnel --stage production",
    "sso": "aws sso login --sso-session=bioacc"
  },
  "devDependencies": {
    "@biomejs/biome": "2.1.1",
    "@clack/prompts": "^0.11.0",
    "@repo/typescript-config": "workspace:*",
    "@turbo/gen": "^2.5.4",
    "@types/aws-lambda": "8.10.150",
    "@types/node": "^24.0.13",
    "commander": "^14.0.0",
    "husky": "^9.1.7",
    "sst": "3.17.10",
    "tsup": "^8.5.0",
    "turbo": "^2.5.4",
    "typescript": "^5.8.3",
    "ultracite": "^5.0.35",
    "vitest": "^3.2.4"
  }
}
